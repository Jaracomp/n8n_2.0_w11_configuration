# n8n Version
N8N_VERSION=2.0.3

# Database Settings (PostgreSQL)
POSTGRES_DB=potgres
POSTGRES_USER=potgres
POSTGRES_PASSWORD=change_this_to_a_secure_password

# Security & Authentication
N8N_ENCRYPTION_KEY=generate_a_long_random_string_here
N8N_RUNNERS_AUTH_TOKEN=generate_another_random_string

# Public Access (ngrok)
NGROK_AUTHTOKEN=your_ngrok_auth_token
NGROK_DOMAIN=your-unique-subdomain.ngrok-free.app

# Custom Variables
CHAT_ID=your_telegram_chat_id
